#include <stdio.h>
#include <string.h>

void printString(const char* str) {
    while (*str != '\0') {
        putchar(*str);
        str++;
    }
}

int test_print_string() {
    const char* message = "Hello World!";
    char expected[14] = "Hello World!";
    
    printf("Running test_print_string...\n");
    
    char buffer[14];
    fflush(stdout);
    setvbuf(stdout, buffer, _IOFBF, sizeof(buffer));
    
    printString(message);
    
    buffer[13] = '\0';
    
    if (strcmp(buffer, expected) == 0) {
        printf("Test Passed!\n");
        return 0;
    } else {
        printf("Test Failed!\n");
        return 1;
    }
}

int main() {
    int result = test_print_string();
    
    return result;
}
